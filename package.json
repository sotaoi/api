{
  "name": "@sotaoi/api",
  "private": false,
  "version": "0.9.6",
  "description": "",
  "deploymentScheme": {
    "init": "npm run deploy:sapi",
    "copy": {
      "skip": false,
      "src": "./deployment",
      "exclude": [
        "./node_modules"
      ]
    },
    "catch": "",
    "conclude": "npm run clean:sapi"
  },
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "bootstrap": "npm install --force --no-audit --no-fund",
    "bootstrap:prod": "npm install --production --force --no-audit --no-fund",
    "deploy:sapi": "node ./scripts/deploy/deploy-sapi",
    "clean:bootstrap": "node ./scripts/clean/clean-bootstrap",
    "clean:sapi": "node ./scripts/clean/clean-sapi"
  },
  "peerDependencies": {
    "@sotaoi/auth": "6.1.3",
    "@sotaoi/config": "*",
    "@sotaoi/omni": "0.9.6"
  },
  "devDependencies": {
    "@sotaoi/api": "file:./",
    "@sotaoi/auth": "file:../sotaoi-auth",
    "@sotaoi/config": "file:../sotaoi-config",
    "@sotaoi/omni": "file:../sotaoi-omni",
    "@types/express": "^4.17.1",
    "@types/node-fetch": "^2.5.10",
    "@types/hapi__hapi": "^19.0.3",
    "@types/hapi__inert": "^5.2.0",
    "@types/hapi__vision": "^5.5.2",
    "@types/hapi-cors": "file:./types/hapi-cors",
    "@types/jsonic": "^0.3.0",
    "@types/lodash": "^4.14.171",
    "@types/node": "^14",
    "@types/uuid": "^8.0.0"
  },
  "dependencies": {
    "@hapi/hapi": "19.1.1",
    "@hapi/inert": "^6.0.3",
    "@hapi/vision": "^6.0.1",
    "chalk": "^4.1.0",
    "express": "4.17.1",
    "flatted": "^3.0.4",
    "hapi-cors": "^1.0.3",
    "joi": "^17.3.0",
    "jsonic": "^1.0.1",
    "knex": "^0.95.6",
    "lodash": "^4.17.21",
    "mongoose": "^5.12.5",
    "mysql": "^2.18.1",
    "node-fetch": "^2.6.1",
    "socket.io": "^3.1.1",
    "ts-node": "^8.10.2",
    "typescript": "4.1.5",
    "uuid": "^8.2.0"
  }
}
